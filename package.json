        "postcss-loader": "^5.2.0",
        "postcss-preset-env": "^6.7.0",
        "react-dev-utils": "^11.0.4",
        "react-dotenv": "^0.1.3",
        "resolve-url-loader": "^3.1.1",
        "sass-loader": "^11.0.1",
        "style-loader": "^2.0.0",
        "typescript": "^4.2.3",
        "url-loader": "^4.1.1",
        "webpack": "^5.24.2",
        "webpack-cli": "^4.5.0",
        "webpack-dev-server": "^3.11.2"
    },
    "dependencies": {
        "classnames": "^2.2.6",
        "react": "^17.0.2",
        "react-dom": "^17.0.2",
        "react-error-boundary": "^3.1.1",
        "react-helmet": "^6.1.0",
        "react-router": "^5.2.0",
        "react-router-dom": "^5.2.0",
        "sass": "^1.49.7"
    },
    "react-dotenv": {
        "whitelist": [
            "CONSUMER_KEY",
            "CONSUMER_SECRET"
        ]
    },
    "scripts": {
        "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
        "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
        "type-check": "tsc --noEmit",
        "start": "react-dotenv && webpack serve --config webpack.config.js --env development --hot --progress --color",
        "build": "react-dotenv && webpack --config webpack.config.js --env production --progress --color"
    }
