        "babel-loader": "^8.1.0",
        "browserslist": "^4.16.5",
        "case-sensitive-paths-webpack-plugin": "^2.3.0",
        "clean-webpack-plugin": "^3.0.0",
        "copy-webpack-plugin": "^8.1.0",
        "css-loader": "^5.0.2",
        "eslint": "^8.8.0",
        "eslint-plugin-react": "^7.20.6",
