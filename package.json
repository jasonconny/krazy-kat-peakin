{
    "name": "krazy-kat-peekin",
    "version": "0.0.1",
    "description": "A lightweight boilerplate for React with TypeScript",
    "repository": "git@github.com:jasonconny/krazy-kat-peekin.git",
    "author": "Jason Conny <jasonconny@gmail.com>",
    "license": "MIT",
    "browserslist": [
        "> 2% in US or last 2 versions",
        "not dead"
    ],
    "devDependencies": {
        "@babel/cli": "^7.10.5",
        "@babel/core": "^7.10.5",
        "@babel/plugin-proposal-class-properties": "^7.10.4",
        "@babel/plugin-proposal-object-rest-spread": "^7.10.4",
        "@babel/plugin-transform-runtime": "^7.10.5",
        "@babel/preset-env": "^7.10.4",
        "@babel/preset-react": "^7.10.4",
        "@babel/preset-typescript": "^7.10.4",
        "@types/react": "^16.9.43",
        "@types/react-dom": "^16.9.8",
        "@typescript-eslint/eslint-plugin": "^3.7.0",
        "@typescript-eslint/parser": "^3.7.0",
        "babel-loader": "^8.1.0",
        "case-sensitive-paths-webpack-plugin": "^2.3.0",
        "clean-webpack-plugin": "^3.0.0",
        "copy-webpack-plugin": "^6.0.3",
        "eslint": "^7.5.0",
        "eslint-loader": "^4.0.2",
        "eslint-plugin-react": "^7.20.3",
        "eslint-plugin-react-hooks": "^4.0.8",
        "file-loader": "^6.0.0",
        "fork-ts-checker-webpack-plugin": "^5.0.7",
        "html-webpack-plugin": "^4.3.0",
        "optimize-css-assets-webpack-plugin": "^5.0.3",
        "postcss-safe-parser": "^4.0.2",
        "react-dev-utils": "^10.2.1",
        "terser-webpack-plugin": "^3.0.7",
        "typescript": "^3.9.7",
        "url-loader": "^4.1.0",
        "webpack": "^4.43.0",
        "webpack-cli": "^3.3.12",
        "webpack-dev-server": "^3.11.0"
    },
    "dependencies": {
        "react": "^16.13.1",
        "react-dom": "^16.13.1"
    },
    "scripts": {
        "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
        "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
        "type-check": "tsc --noEmit",
        "start": "NODE_ENV=development webpack-dev-server --config webpack.config.js --env.target=local --env.watch --progress --colors",
        "build": "webpack --config webpack.config.js --env.production --progress --colors"
    }
}
