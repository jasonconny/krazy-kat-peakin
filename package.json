{
    "name": "krazy-kat-peekin",
    "version": "0.0.1",
    "description": "A lightweight boilerplate for React with TypeScript",
    "repository": "git@github.com:jasonconny/krazy-kat-peekin.git",
    "author": "Jason Conny <jasonconny@gmail.com>",
    "license": "MIT",
    "browserslist": [
        "> 2% in US or last 2 versions",
        "not dead"
    ],
    "dependencies": {
        "react": "^16.13.1",
        "react-dom": "^16.13.1"
    },
    "scripts": {
        "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
        "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
        "type-check": "tsc --noEmit",
        "start": "webpack-dev-server --config webpack.config.js --env.development --env.watch --progress --colors",
        "build": "webpack --config webpack.config.js --env.production --progress --color"
    }
}
